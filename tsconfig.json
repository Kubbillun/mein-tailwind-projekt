{
    "compilerOptions": {
        "jsx": "react-jsx",
        "moduleResolution": "Bundler",
        "target": "ES2020",
        "module": "ESNext",
        "strict": true,
        "baseUrl": "./src",
        "paths": {
            "*": [
                "*"
            ]
        },
        "resolveJsonModule": true,
        "esModuleInterop": true
    },
    "include": [
        "src"
    ]
}