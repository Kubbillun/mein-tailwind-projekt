{
  "name": "quickstart-crew",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run predev && vite --port 5173 --strictPort",
    "build": "vite build",
    "preview": "vite preview",
    "publish:now": "dotenv -e .env -- node scripts/publish-now.mjs",
    "publish:in": "dotenv -e .env -- node scripts/publish-in.mjs",
    "publish:backfill": "dotenv -e .env -- node scripts/publish-backfill.mjs",
    "rpc:smoke": "dotenv -e .env -- node scripts/rpc-smoke.mjs",
    "lint": "echo \"(temporary) no lint configured\" && exit 0",
    "test": "node test.js",
    "feed:check": "node --env-file=.env.local scripts/feed_check.mjs",
    "feed:smoke:search": "node --env-file=.env.local scripts/feed_smoke_search.mjs",
    "feed:smoke:page": "node --env-file=.env.local scripts/feed_smoke_pagination.mjs",
    "env:print": "node -e \"import(\\\"vite\\\").then(v=>{const e=v.loadEnv(\\\"development\\\", process.cwd(), \\\"\\\"); console.log({ VITE_PROJECT_REF:e.VITE_PROJECT_REF, HAS_KEY: !!e.VITE_SB_ANON_KEY });})\"",
    "dev:clean": "pkill -f vite 2>/dev/null || true && rm -rf node_modules/.vite && npm run dev -- --force",
    "predev": "kill-port 5173 || true"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^10.0.0",
    "kill-port": "^2.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "vite": "^7.1.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3",
    "zod": "3.25.76"
  }
}
